local function createGUI()

    local oldGui = player.PlayerGui:FindFirstChild("TuanMM2Fram")
    if oldGui then oldGui:Destroy() end

    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "TuanMM2Fram"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = player.PlayerGui

    local Background = Instance.new("ImageLabel")
    Background.Size = UDim2.new(0, 250, 0, 250)
    Background.Position = UDim2.new(0.5, -125, 0.5, -125)
    Background.BackgroundTransparency = 1
    Background.Image = "rbxassetid://101594287578142" -- hình nền mờ
    Background.ImageColor3 = Color3.fromRGB(255, 255, 255)
    Background.ImageTransparency = 0.7 -- mờ
    Background.Parent = ScreenGui

    local Diamond = Instance.new("ImageLabel")
    Diamond.Size = UDim2.new(0, 200, 0, 200)
    Diamond.Position = UDim2.new(0.5, -100, 0.5, -100)
    Diamond.BackgroundTransparency = 1
    Diamond.Image = "rbxassetid://6023426923"
    Diamond.ImageColor3 = Color3.fromRGB(50, 50, 50)
    Diamond.Parent = ScreenGui
    Diamond.Active = true
    Diamond.Draggable = true

    -- Title Text
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 40)
    Title.Position = UDim2.new(0, 0, 0.25, 0)
    Title.BackgroundTransparency = 1
    Title.Text = "TuanMM2Fram"
    Title.TextColor3 = Color3.new(1, 1, 1)
    Title.TextSize = 18
    Title.Font = Enum.Font.SourceSansBold
    Title.Parent = Diamond

    -- Toggle button
    local ToggleButton = Instance.new("TextButton")
    ToggleButton.Size = UDim2.new(0.6, 0, 0, 40)
    ToggleButton.Position = UDim2.new(0.2, 0, 0.55, 0)
    ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    ToggleButton.Text = "Teleport OFF"
    ToggleButton.TextColor3 = Color3.new(1, 1, 1)
    ToggleButton.TextSize = 14
    ToggleButton.Font = Enum.Font.SourceSansBold
    ToggleButton.Parent = Diamond

    ToggleButton.MouseButton1Click:Connect(function()
        teleportEnabled = not teleportEnabled
        if teleportEnabled then
            ToggleButton.Text = "Teleport ON"
            ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        else
            ToggleButton.Text = "Teleport OFF"
            ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        end
    end)

    return ScreenGui
end
