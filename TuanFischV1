local KEY = "TuanFisch2025"
local key = ""

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "TuanFich Hub.   [ Premium ]",
    SubTitle = "By : Tuansidacon",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 340),
    Acrylic = true,
    Theme = "White",
    MinimizeKey = Enum.KeyCode.LeftAlt
})

local Tabs = {
    KeySys = Window:AddTab({ Title = "Key System", Icon = "key" }),
}

local Entkey = Tabs.KeySys:AddInput("Input", {
    Title = "Enter Key",
    Description = "Enter Key Here",
    Default = "",
    Placeholder = "Enter key…",
    Numeric = false,
    Finished = false,
    Callback = function(Value)
        key = Value
    end
})

local Checkkey = Tabs.KeySys:AddButton({
    Title = "Check Key",
    Description = "Enter Key before pressing this button",
    Callback = function()
        if key == KEY then
            print("Key is valid")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/phamtuananh24112014-cloud/tuandelta/refs/heads/main/TuanFischV1"))()
        else
            print("Key sai rồi hẹ hẹ")
        end
    end
})

local GetDiscord = Tabs.KeySys:AddButton({
    Title = "Get Key on Discord",
    Description = "Copy Discord link to get the key",
    Callback = function()
        setclipboard("https://discord.gg/qus6dfqw")
    end
})

Window:SelectTab(1)
